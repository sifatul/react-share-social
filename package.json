{
  "name": "react-share-social",
  "homepage": "http://sifatul.github.io/react-share-social",
  "description": "UI component to share contents in Social media. Share contents in facebook, twitter, pinterest, whatsapp etc.",
  "version": "0.1.59",
  "keywords": [
    "react",
    "component",
    " react-component",
    "social",
    "media",
    "social-media",
    "share",
    "button",
    "material design",
    "material-ui"
  ],
  "author": "Md Sifatul Islam <devsifat@gmail.com> (http://sifatul.github.io/)",
  "private": false,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.19",
    "@mui/styles": "^5.15.19",
    "gh-pages": "^6.1.1",
    "jest": "28",
    "npm-check-updates": "^16.14.20",
    "react-share": "^5.1.0",
    "typescript": "4"
  },
  "scripts": {
    "npm:publish": "tsc",
    "predeploy": "npm run build && npm run test",
    "deploy": "npm run predeploy && gh-pages -d build",
    "start": "react-scripts start",
    "update:check": "ncu",
    "update:upgrade": "ncu -u",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc",
    "publish:window": "cross-env NODE_ENV=production && npx rimraf dist && mkdir dist &&  babel src/components -d dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^4.0.1"
  },
  "peerDependencies":{
    "react": "^18.3.1",
  }
}
